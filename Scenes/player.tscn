[sub_resource type="GDScript" id="GDScript_6t5aa"]
script/source = "extends Camera2D

@export var shake_strength := 8.0
@export var shake_decay := 20.0

var shake_amount := 0.0
var original_offset := Vector2.ZERO

func _ready():
	original_offset = offset

func _process(delta):
	if shake_amount > 0:
		shake_amount = move_toward(shake_amount, 0, shake_decay * delta)
		offset = original_offset + Vector2(
			randf_range(-shake_amount, shake_amount),
			randf_range(-shake_amount, shake_amount)
		)
	else:
		offset = original_offset

func shake(strength := 8.0):
	shake_amount = strength
"

